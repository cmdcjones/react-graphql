{
  "name": "react-graphql",
  "private": true,
  "version": "0.0.0",
  "description": "A React application using a GraphQL API and Prisma (PostgreSQL) on the backend.",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch \"src\/**\" --ext \"ts,json\" --ignore \"src\/**\/*.spec.ts\" --exec \"ts-node-esm src\/index.ts\"",
    "compile": "tsc"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@prisma/client": "^4.8.1",
    "graphql": "^16.6.0",
    "nexus": "^1.3.0"
  },
  "prisma": {
    "seed": "ts-node-esm prisma/seed.ts"
  }
}
